	      .ipl-news.event-news ul.list2 li{margin-bottom: 10px}
	      .ipl-news.event-news ul.list2 li span{overflow: hidden; height: 29px; display: block}
	      .ipl-news.event-news ul.list2 li:nth-child(5) ~ li{display: none}
	      .ipl-news.event-news h2 { margin-bottom:5px; }
	      .ipl-news.event-news .widget-heading .more { margin-top:4px; text-transform:uppercase; }
	      
	      @media screen and (max-width: 999px){
	      .orange-purple-list{width: 166px}
